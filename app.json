      "edgeToEdgeEnabled": true,
      "package": "com.pony.kuzushi",
      "versionCode": 3,
      "permissions": [
        "INTERNET",
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7911846990308189~6339489042",
          "iosAppId": "ca-app-pub-7911846990308189~6339489042"
